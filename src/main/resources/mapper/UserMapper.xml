<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.coordinate_post.repository.UserMapper">
    <select id="findUsers" resultType="com.example.coordinate_post.dto.UserDto">
        SELECT id, account_id, user_name, profile_image_url
        FROM users
        ORDER BY id ASC
        LIMIT #{limit} OFFSET #{offset}
    </select>
</mapper>